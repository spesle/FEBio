name: Do things
on: push
jobs:
  job1:
    runs-on: ubuntu-latest
    outputs:
      url: ${{ steps.step1.outputs.url }} # map step output to job output
    steps:
      - id: step1
        name: send url to other job
        run: |
          echo "::set-output name=url::https://google.com"

  job2:
    runs-on: ubuntu-latest
    needs: job1
    steps:
      - id: step1
        run : |
          echo "${{ needs.job1.outputs.url }}" # grab job output here
